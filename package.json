    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/express": "^4.17.21",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "husky": "^8.0.0",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^7.9.0"
  }
}
